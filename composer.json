{
    "name": "univpancasila/storage-up",
    "description": "A Laravel package developed by the Internal Organization of the University of Pancasila, designed to simplify file storage management. This package provides an intuitive facade for uploading, retrieving, and managing files across organizational applications through a unified API.",
    "keywords": [
        "univpancasila",
        "laravel",
        "storage-up"
    ],
    "homepage": "https://github.com/univpancasila/storageup-facade",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdan Syakuro",
            "email": "abdan@univpancasila.ac.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1|^7.10.0|^6.4",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0|^8.22.0|^7.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Univpancasila\\StorageUp\\": "src/",
            "Univpancasila\\StorageUp\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Univpancasila\\StorageUp\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Univpancasila\\StorageUp\\StorageUpServiceProvider"
            ],
            "aliases": {
                "StorageUp": "Univpancasila\\StorageUp\\Facades\\StorageUp"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
